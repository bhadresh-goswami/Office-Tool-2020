@model IEnumerable<DashReportingTool.Models.StatusMaster>

@{
    ViewBag.Title = "Status List";
    Layout = "~/Areas/Manage/Views/HomeManage/Index.cshtml";
}


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table dttable">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StatusTitle)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StatusTitle)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StatusId }) |
            @Html.ActionLink("Details", "Details", new { id=item.StatusId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StatusId })
        </td>
    </tr>
}

</table>
